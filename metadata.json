{
    "name": "puppet-elasticsearch_backup",
    "version": "1.0.0",
    "author": "Skyscrapers",
    "summary": "Puppet Elasticsearch backup module",
    "license": "Apache-2.0",
    "source": "https://github.com/skyscrapers/puppet-elasticsearch_backup",
    "project_page": "https://github.com/skyscrapers/puppet-elasticsearch_backup",
    "issues_url": "https://github.com/skyscrapers/puppet-elasticsearch_backup/issues",
    "dependencies": [
        { "name": "puppetlabs/stdlib", "version_requirement": ">= 3.2.0 < 5.0.0" }
    ],
    "requirements": [{
        "name": "puppet",
        "version_requirement": "4.x"
    }],
    "operatingsystem_support": [{
